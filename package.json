{
  "name": "bootstrap-npm-starter",
  "version": "1.4.0",
  "private": true,
  "description": "Demo project for building a Bootstrap powered site via npm.",
  "author": "mdo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twbs/bootstrap-npm-starter.git"
  },
  "bugs": {
    "url": "https://github.com/twbs/bootstrap-npm-starter/issues"
  },
  "homepage": "https://github.com/twbs/bootstrap-npm-starter#readme",
  "scripts": {
    "build": "parcel build --public-url ./ ./src/index.html --dist-dir build",
    "css-lint": "stylelint \"**/*.scss\"",
    "prebuild": "rimraf build",
    "start": "parcel serve ./src/index.html",
    "test": "npm-run-all css-lint"
  },
  "keywords": [
    "bootstrap",
    "sass",
    "css",
    "javascript",
    "starter",
    "template"
  ],
  "dependencies": {
    "@popperjs/core": "^2.9.3",
    "bootstrap": "^5.1.0",
    "bootstrap-icons": "^1.5.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@parcel/transformer-sass": "^2.0.0-rc.0",
    "autoprefixer": "^10.3.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-rc.0",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.2",
    "postcss-url": "^10.1.3",
    "rimraf": "^3.0.2",
    "sass": "^1.38.2",
    "stylelint": "^13.13.1",
    "stylelint-config-twbs-bootstrap": "^2.2.3"
  }
}
